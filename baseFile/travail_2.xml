<?xml version="1.0"?>
<!DOCTYPE rdf:RDF
	[<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">]>
<rdf:RDF
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:foaf="http://xmlns.com/foaf/0.1/"
	xmlns:classes="http://www.example.com/classes#"
	xmlns:properties="http://www.example.com/properties#"
	xmlns:localisation="http://www.example.com/localisation#"
	xml:base="http://www.example.com/base">

	<classes:Demigod rdf:ID="Hercule">
		<foaf:name>Hercule</foaf:name>
		<properties:accomplish rdf:resource="http://www.example.com/base#Tache_6"/>
	</classes:Demigod>

	<classes:Task rdf:ID="Tache_6">
		<foaf:name>Tuer les oiseaux du lac de Stymphale</foaf:name>
		<properties:requester rdf:resource="http://www.example.com/base#Euristhée"/>
		<properties:task_reason rdf:resource="http://www.example.com/base#Raison_tache_6"/>
		<properties:is_localised rdf:resource="https://fr.wikipedia.org/wiki/Lac_Stymphale#/maplink/1"></properties:is_localised>
		<properties:task_requirement rdf:resource="http://www.example.com/base#Objectif_tache_6"/>
		<properties:task_step rdf:resource="http://www.example.com/base#Etape_tache_6"/>
	</classes:Task>

	<classes:Task rdf:ID="Raison_tache_6">
		<properties:reasons_list>
			<rdf:Bag>
				<rdf:li rdf:resource="http://www.example.com/properties#Empoisonnement_des_environs"/>
			</rdf:Bag>
		</properties:reasons_list>
	</classes:Task>

	<classes:Task_requirements rdf:ID="Objectif_tache_6">
		<properties:require>
			<rdf:Bag>
				<rdf:li rdf:resource="http://www.example.com/base#Objectif_tache_6_1"/>
			</rdf:Bag>
		</properties:require>
	</classes:Task_requirements>

	<properties:Goal rdf:ID="Objectif_tache_6_1">
		<properties:kill rdf:resource="http://www.example.com/base#Hydre_de_Lerne"/>
	</properties:Goal>
	
	<classes:Taks_steps rdf:ID="Etape_tache_6">
		<properties:step_list>
			<rdf:Bag>
				<rdf:li rdf:resource="http://www.example.com/properties#Se_proteger_les_voies_respiratoires"/>
				<rdf:li rdf:resource="http://www.example.com/properties#Tirer_des_flèches_enflammées"/>
				<rdf:li rdf:resource="http://www.example.com/properties#Couper_tete_hydre_sans_succès"/>
				<rdf:li rdf:resource="http://www.example.com/properties#Demander_aide_à_Iolaos"/>
				<rdf:li rdf:resource="http://www.example.com/properties#Couper_et_cotériser_les_tètes_qui_repoussent"/>
				<rdf:li rdf:resource="http://www.example.com/properties#Tremper_flèche_dans_sang_hydre"/>
			</rdf:Bag>
		</properties:step_list>
	</classes:Taks_steps>

</rdf:RDF>